  "scripts": {
    "build": "craco build",
    "start": "rm -rf .env && cp .env.local.dev .env && craco start",
    "lint": "eslint \"src/**/*.js\"",
    "prettier": "npx prettier --write 'src/**/*.{js,ts,tsx}'",
    "test": "craco test",
    "test:cov": "npm test -- --coverage --watchAll=false",
    "test:debug": "craco --inspect-brk test --runInBand",
    "find-deadcode": "ts-prune | (! grep -v 'used in module')",
    "deploy:prod": "rm -rf .env.local build && cp .env.local.production .env && craco build && firebase deploy && rm -rf .env"
  },
